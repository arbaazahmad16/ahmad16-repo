pipeline{
    agent any
	environment{
		PATH = "/opt/apache-maven-3.9.9/bin:$PATH"
	}
    stages{
        stage("Git checkout"){
            steps{
                git branch: 'main', changelog: false, poll: false, url: 'https://github.com/arbaazahmad16/ahmad16-repo'
            }
        }
	stage("Maven Build"){
		steps{
			sh "mvn clean package"
		}	
	}
    }
	post {
        success {
            echo 'Pipeline ia successful!!!'
        }
        failure {
            echo 'Pipeline is failed.'
        }
    }
}
